{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ]
}